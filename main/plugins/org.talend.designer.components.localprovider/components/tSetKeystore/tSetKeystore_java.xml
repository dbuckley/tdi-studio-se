<COMPONENT>
	<HEADER PLATEFORM="ALL" SERIAL="" VERSION="0.102" STATUS="ALPHA"
		COMPATIBILITY="ALL" AUTHOR="Talend" RELEASE_DATE="20050320A"
		STARTABLE="true">
		<SIGNATURE />
	</HEADER>

  <FAMILIES>
    <FAMILY>Internet</FAMILY>
  </FAMILIES>

	<DOCUMENTATION>
		<URL />
	</DOCUMENTATION>

  <CONNECTORS>
    <CONNECTOR CTYPE="FLOW" MAX_INPUT="0" MAX_OUTPUT="0"/>
    <CONNECTOR CTYPE="ITERATE" MAX_OUTPUT="0" MAX_INPUT="0"/>
    <CONNECTOR CTYPE="SUBJOB_OK" MAX_INPUT="1" />
    <CONNECTOR CTYPE="SUBJOB_ERROR" MAX_INPUT="1" />
    <CONNECTOR CTYPE="COMPONENT_OK"/>
    <CONNECTOR CTYPE="COMPONENT_ERROR"/>
    <CONNECTOR CTYPE="RUN_IF"/>
  </CONNECTORS>

	<PARAMETERS>
		<PARAMETER NAME="SET_TRUSTSTORE" FIELD="RADIO" NUM_ROW="5" GROUP="STORE_TYPE">
			<DEFAULT>true</DEFAULT>
		</PARAMETER>

		<!-- keep the old name to avoid migration -->
		<PARAMETER NAME="NEED_CLIENT_AUTH" FIELD="RADIO" NUM_ROW="5" GROUP="STORE_TYPE">
			<DEFAULT>false</DEFAULT>
		</PARAMETER>

		<PARAMETER NAME="SET_TRUSTSTORE_KEYSTORE" FIELD="RADIO" NUM_ROW="5" GROUP="STORE_TYPE">
			<DEFAULT>false</DEFAULT>
		</PARAMETER>

		<PARAMETER NAME="TRUSTSTORE_TYPE" FIELD="CLOSED_LIST" SHOW_IF="(SET_TRUSTSTORE == 'true') OR (SET_TRUSTSTORE_KEYSTORE == 'true')" NUM_ROW="10">
			<ITEMS DEFAULT="JKS">
				<ITEM NAME="JKS"   VALUE="JKS" />
				<ITEM NAME="PKCS12"   VALUE="PKCS12" />
			</ITEMS>
		</PARAMETER>
		<PARAMETER NAME="SSL_TRUSTSERVER_TRUSTSTORE" FIELD="FILE" SHOW_IF="(SET_TRUSTSTORE == 'true') OR (SET_TRUSTSTORE_KEYSTORE == 'true')" NUM_ROW="20">
			<DEFAULT>"C:/clientTrustStore.trustStore"</DEFAULT>
		</PARAMETER>
		<PARAMETER NAME="SSL_TRUSTSERVER_PASSWORD" FIELD="PASSWORD" SHOW_IF="(SET_TRUSTSTORE == 'true') OR (SET_TRUSTSTORE_KEYSTORE == 'true')" NUM_ROW="30">
			<DEFAULT>"password"</DEFAULT>
		</PARAMETER>
		
		<PARAMETER NAME="KEYSTORE_TYPE" FIELD="CLOSED_LIST" SHOW_IF="(NEED_CLIENT_AUTH == 'true') OR (SET_TRUSTSTORE_KEYSTORE == 'true')" NUM_ROW="50">
			<ITEMS DEFAULT="JKS">
				<ITEM NAME="JKS"   VALUE="JKS" />
				<ITEM NAME="PKCS12"   VALUE="PKCS12" />
			</ITEMS>
		</PARAMETER>
		<PARAMETER NAME="SSL_KEYSTORE" FIELD="FILE" NUM_ROW="60" SHOW_IF="(NEED_CLIENT_AUTH == 'true') OR (SET_TRUSTSTORE_KEYSTORE == 'true')">
			<DEFAULT>"C:/client.keystore"</DEFAULT>
		</PARAMETER>
		<PARAMETER NAME="SSL_KEYSTORE_PASSWORD" FIELD="PASSWORD" NUM_ROW="70" SHOW_IF="(NEED_CLIENT_AUTH == 'true') OR (SET_TRUSTSTORE_KEYSTORE == 'true')">
			<DEFAULT>"password"</DEFAULT>
		</PARAMETER>
		
		<PARAMETER NAME="VERIFY_NAME" FIELD="CHECK" REQUIRED="true" NUM_ROW="80" SHOW="true"> 
			<DEFAULT>true</DEFAULT>
		</PARAMETER>
		
		<PARAMETER NAME="USE_SSLCONTEXT" FIELD="CHECK" REQUIRED="true" NUM_ROW="90" SHOW="true"> 
			<DEFAULT>false</DEFAULT>
		</PARAMETER>
	</PARAMETERS>
		
	<CODEGENERATION>
		<IMPORTS>
			<IMPORT NAME="sslcontext-kickstart" MODULE="sslcontext-kickstart-7.4.11.jar" MVN="mvn:io.github.hakky54/sslcontext-kickstart/7.4.11" REQUIRED_IF="USE_SSLCONTEXT == 'true'" />
		</IMPORTS>
	</CODEGENERATION>
	
	<RETURNS/>
</COMPONENT>